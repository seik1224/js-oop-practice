{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": ["standard", "prettier"],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ]
  },
  "plugins": ["prettier"]
}
